<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_CENTER" fillWidth="false" minHeight="580.0" minWidth="500.0" prefHeight="580.0" prefWidth="500.0" spacing="20.0" style="-fx-padding: 0; -fx-spacing: 10; -fx-background-color: #e0e0e0;" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ter.client.ClientController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <HBox alignment="CENTER" minHeight="80.0" minWidth="500.0" prefHeight="80.0" prefWidth="500.0" styleClass="top_bar" stylesheets="@style.css">
         <children>
            <Label fx:id="topText" alignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="Arial" size="24.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <GridPane fx:id="table" alignment="CENTER" disable="false" styleClass="table_game" stylesheets="@style.css">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <StackPane alignment="CENTER" onMouseClicked="#play" prefHeight="200.0" prefWidth="200.0" styleClass="table_panel_game" stylesheets="@style.css" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </StackPane>
            <StackPane alignment="CENTER" onMouseClicked="#play" prefHeight="200.0" prefWidth="200.0" styleClass="table_panel_game" stylesheets="@style.css" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </StackPane>
            <StackPane alignment="CENTER" onMouseClicked="#play" prefHeight="200.0" prefWidth="200.0" styleClass="table_panel_game" stylesheets="@style.css" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </StackPane>
            <StackPane alignment="CENTER" onMouseClicked="#play" prefHeight="200.0" prefWidth="200.0" styleClass="table_panel_game" stylesheets="@style.css" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </StackPane>
            <StackPane alignment="CENTER" onMouseClicked="#play" prefHeight="200.0" prefWidth="200.0" styleClass="table_panel_game" stylesheets="@style.css" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </StackPane>
            <StackPane alignment="CENTER" onMouseClicked="#play" prefHeight="200.0" prefWidth="200.0" styleClass="table_panel_game" stylesheets="@style.css" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </StackPane>
            <StackPane alignment="CENTER" onMouseClicked="#play" prefHeight="200.0" prefWidth="200.0" styleClass="table_panel_game" stylesheets="@style.css" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </StackPane>
            <StackPane alignment="CENTER" onMouseClicked="#play" prefHeight="200.0" prefWidth="200.0" styleClass="table_panel_game" stylesheets="@style.css" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </StackPane>
            <StackPane alignment="CENTER" onMouseClicked="#play" prefHeight="200.0" prefWidth="200.0" styleClass="table_panel_game" stylesheets="@style.css" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </StackPane>
         </children>
      </GridPane>
   </children>
</VBox>
